{
    "manifest_version": 2,
    "name": "TabLoader",
    "version": "0.1.0",
  
    "description": "An extension which supports saving every tabs from browser window and loading them at once",
    "icons": {
      "48": "icons/icon.png"
    },
  
    "permissions": [
      "bookmarks",
      "tabs"
    ],
  
    "browser_action": {
      "default_title": "TabLoader",
      "default_popup": "/popup/index.html"
    }
  }
  